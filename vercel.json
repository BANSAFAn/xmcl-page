{
    "cleanUrls": true,
    "rewrites": [
        {
            "source": "/docx",
            "destination": "https://docs.xmcl.app"
        }
    ],
    "headers": [
        {
            "source": "/.well-known/microsoft-identity-association.json",
            "headers": [
                {
                    "key": "Content-Type",
                    "value": "application/json"
                }
            ]
        }
    ]
}